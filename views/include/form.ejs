<form action="/api/users" method="POST" id="add_user">
    <div class="new_user">
        <div class="form-group">
            <label for="name" class="text-light">Name</label>
            <input type="hidden" name="id" value="">
            <input type="text" name="name" value="" placeholder="Mark Stoenis">
        </div>
        <div class="form-group">
            <label for="Email" class="text-light">Email</label>
            <input type="text" name="email" value="" placeholder="example@gmail.com">
        </div>
        <div class="form-group">
            <label for="Contact" class="text-light">Contact</label>
            <input type="tel" name="contact" value="" placeholder="+91XXXXXXXX">

            
        </div>
        
        <div class="form-group">
            <label for="Address" class="text-light">Address</label>
            <!-- <input type="text" name="tel" value="" placeholder="+91XXXXXXXX"> -->
            <input type="text" name="address" value="" placeholder="H.NO.- XYZ, STREET NO. XYZ">

                
        </div>

        <div class="form-group">
            <button type="submit" class="btn text-dark update">Save</button>
        </div>

    </div>
</form>

